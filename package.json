{
  "name": "vridge-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false --maxWorkers=50%",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "test:mutation": "stryker run",
    "test:mutation:incremental": "stryker run --incremental",
    "test:flaky": "node scripts/flaky-test-detector.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "perf:analyze": "node scripts/bundle-analysis.js",
    "lighthouse:ci": "lhci autorun",
    "perf:lighthouse": "lhci autorun",
    "perf:monitor": "node scripts/performance-monitor.js",
    "perf:test": "npm run build && npm run perf:analyze && npm run perf:lighthouse && npm run perf:monitor",
    "accessibility": "axe http://localhost:3000 --exit",
    "accessibility:test": "jest --testPathPattern=accessibility.test.ts",
    "quality:dashboard": "node scripts/quality-dashboard.js",
    "quality:full": "npm run test:coverage && npm run test:mutation:incremental && npm run perf:test && npm run accessibility:test",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "e2e:ci": "start-server-and-test start http://localhost:3000 'cypress run --record'",
    "e2e:comprehensive": "node scripts/run-comprehensive-e2e.js",
    "e2e:journey": "cypress run --spec 'cypress/e2e/complete-user-journey.cy.ts'",
    "e2e:accessibility": "cypress run --spec 'cypress/e2e/accessibility-compliance.cy.ts'",
    "e2e:performance": "cypress run --spec 'cypress/e2e/performance-monitoring.cy.ts'",
    "e2e:cross-browser": "cypress run --spec 'cypress/e2e/cross-browser-compatibility.cy.ts'",
    "e2e:parallel": "cypress run --record --parallel",
    "contract:test": "jest --testPathPattern=pact-contract.test.ts",
    "contract:publish": "npx pact-broker publish pacts --consumer-app-version=$npm_package_version --broker-base-url=$PACT_BROKER_URL",
    "data:quality": "node scripts/data-quality-gate.js",
    "data:quality:dev": "node scripts/data-quality-gate.js --env=development --verbose",
    "data:quality:staging": "node scripts/data-quality-gate.js --env=staging",
    "data:quality:prod": "node scripts/data-quality-gate.js --env=production",
    "data:quality:dry-run": "node scripts/data-quality-gate.js --dry-run --verbose",
    "data:integrity": "jest --testPathPattern=data-integrity-checker.test.ts",
    "data:pipeline": "jest --testPathPattern=data-quality-pipeline.test.ts",
    "data:test": "npm run data:integrity && npm run data:pipeline",
    "ci:quality": "npm run lint && npm run test:coverage && npm run build && npm run contract:test && npm run data:quality",
    "ci:data": "npm run data:test && npm run data:quality",
    "deploy:health": "node scripts/deployment-health-check.js",
    "deploy:verify": "node scripts/deployment-health-check.js full",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@reduxjs/toolkit": "~2.0.1",
    "axios": "^1.11.0",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "immer": "^10.1.3",
    "msw": "^2.11.1",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-redux": "^9.2.0",
    "web-vitals": "^5.1.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/grep": "^4.1.1",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@pact-foundation/pact": "^15.0.1",
    "@pact-foundation/pact-node": "^10.18.0",
    "@percy/cli": "^1.31.1",
    "@percy/cypress": "^3.1.6",
    "@stryker-mutator/core": "^9.1.1",
    "@stryker-mutator/jest-runner": "^9.1.1",
    "@stryker-mutator/typescript-checker": "^9.1.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-jest": "^30.1.2",
    "commitlint": "^19.8.1",
    "cypress": "^15.1.0",
    "cypress-axe": "^1.7.0",
    "cypress-real-events": "^1.14.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-boundaries": "^5.0.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "start-server-and-test": "^2.0.13",
    "tailwindcss": "^4",
    "typescript": "^5",
    "undici": "^7.15.0",
    "wait-on": "^8.0.4"
  }
}
