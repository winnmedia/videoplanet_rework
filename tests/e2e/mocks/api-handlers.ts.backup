      }),
      MockDataGenerator.createCalendarEvent({
        title: 'Event Shoot', // ê°™ì€ ë‚ ì§œë¡œ ì¶©ëŒ ìƒì„±
        type: 'shooting',
        projectColor: '#F59E0B',
